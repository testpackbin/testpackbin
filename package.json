{
  "name": "testpackbin",
  "version": "0.0.01",
  "description": "An extension of the excellent webpackbin",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/testpackbin/testpackbin.git"
  },
  "keywords": [
    "express",
    "webpack",
    "node",
    "cerebral"
  ],
  "author": "Andrew Kemp, Brett Caudill, Kirill Prostyakov, Zach Parduhn, Zac Anger",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/testpackbin/testpackbin/issues"
  },
  "homepage": "https://github.com/testpackbin/testpackbin.git",
  "scripts": {
    "test": "NODE_ENV=test mocha test/spec/",
    "start": "concurrently \"npm run client\" \"npm run server\"",
    "client": "cross-env kotatsu serve app/main.jsx --port 3000 --index index.html --config webpack.config.js --devtool eval-source-map --proxy /api http://localhost:3999/api",
    "server": "cross-env kotatsu start start.js --source-maps",
    "build": "concurrently \"npm run client:build\" \"npm run server:build\"",
    "client:build": "cross-env NODE_ENV=production kotatsu build client app/main.js --config webpack.production.config.js --minify -o dist/client_bundle.js ",
    "server:build": "cross-env NODE_ENV=production kotatsu build server start.js --config webpack.production.config.js --minify -o dist/server_bundle.js ",
    "seed": "node seed.js"
  },
  "dependencies": {
    "babel-core": "^6.7.6",
    "baobab": "^2.3.3",
    "bluebird": "^3.3.5",
    "body-parser": "^1.15.0",
    "cerebral": "^0.33.24",
    "cerebral-addons": "^0.4.10",
    "cerebral-model-baobab": "^0.4.7",
    "cerebral-module-devtools": "^0.5.3",
    "cerebral-module-http": "^0.1.1",
    "cerebral-module-recorder": "^0.5.7",
    "cerebral-module-router": "^0.12.5",
    "cerebral-view-react": "^0.11.6",
    "compression": "^1.6.1",
    "cors": "^2.7.1",
    "dotenv": "^2.0.0",
    "errorhandler": "^1.4.3",
    "express": "^4.13.4",
    "express-jwt": "^3.3.0",
    "jsonwebtoken": "^5.7.0",
    "lodash": "^4.11.1",
    "mongoose": "^4.4.12",
    "morgan": "^1.7.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  },
  "devDependencies": {
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "chai-http": "^2.0.1",
    "concurrently": "^2.0.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "faker": "^3.1.0",
    "kotatsu": "^0.10.0",
    "style-loader": "^0.13.0"
  }
}
